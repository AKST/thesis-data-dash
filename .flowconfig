[ignore]
.*/node_modules/config-chain/test/broken.json
.*/node_modules/vue/
.*/node_modules/vue-router/
.*/node_modules/npm/test/fixtures/config/package.json
.*/node_modules/npmconf/test/fixtures/package.json

[include]

[libs]

[options]
suppress_comment= \\(.\\|\n\\)*\\$SuppressFlow
module.name_mapper='src/\(.*\)' -> '<PROJECT_ROOT>/\1'
module.system.node.resolve_dirname=src
module.system.node.resolve_dirname=node_modules
unsafe.enable_getters_and_setters=false
